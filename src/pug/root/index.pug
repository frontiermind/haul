extends ../_template/_extend

block vars
  -
      var page = {
          root: './',
          class: 'page-home'
      };
      const breakpoint = "768px"

block content
    section.visual.js-menu-scroll
        .visual__cont
            .visual__nav.visual-nav.js-nav-txt
                a(href="./#mission").visual-nav__txt
                    | Mission
                a(href="./#service").visual-nav__txt
                    | Service
                a(href="./#company").visual-nav__txt
                    | Company
                a(href="./#recruit").visual-nav__txt
                    | Recruit
                a(href="./contact").visual-nav__txt
                    | Contact

            .visual__text-cont
                p.visual__text.js-visual-txt
                    | Envision your ideal.
                    br
                    | Change the structure;
                    br
                    | integrate contradictions and conflicts.
            .visual__logo
                .visual-logo
                    .visual-logo__wrapper.visual-logo__wrapper--logo1
                        img.visual-logo__img.js-logo-img(src="./images/visual/top_visual_logo_01.svg" alt="H")
                    .visual-logo__wrapper.visual-logo__wrapper--logo2
                        img.visual-logo__img.js-logo-img(src="./images/visual/top_visual_logo_02.svg" alt="a")
                    .visual-logo__wrapper.visual-logo__wrapper--logo3
                        img.visual-logo__img.js-logo-img(src="./images/visual/top_visual_logo_03.svg" alt="u")
                    .visual-logo__wrapper.visual-logo__wrapper--logo4
                        img.visual-logo__img.js-logo-img(src="./images/visual/top_visual_logo_04.svg" alt="l")


    section#mission.top-mission
        .top-mission__cont
            h2.c-ttl
                | Mission
            p.top-mission__intro
                | やりたいことがない
                br
                | という世界から、
                br
                | やりたいことがある
                br
                | と言える日本にすること。
            p.top-mission__txt
                | 私たちは、ソフトウェアの世界で、日本人が日本製のプロダクトを誇れるようになることを目指しています。エンジニアの底上げ、日本の生産性向上などを行い、人々の自己改革を容易にし、個のモビリティを高めること、人材の素材適所を実現させることで、日本発のプロダクトを世界に発信できる土壌作りに貢献します。
        picture.top-mission__img-cont
            source(srcset="./images/mission/top_mission_pc_01.jpg" media=`(min-width: ${breakpoint})`)
            img(src="./images/mission/top_mission_sp_01.jpg").top-mission__img

    section#service.top-service
        .top-service__cont
            h2.c-ttl
                | Service
                .top-service__sub-cont
